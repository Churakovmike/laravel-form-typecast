{
    "name": "churakovmike/laravel-form-typecast",
    "type": "library",
    "description": "Laravel request form type casting",
    "keywords": [
        "laravel",
        "typecast",
        "typecasting"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Churakov Mike"
        }
    ],
    "require": {
        "illuminate/view": "^5.5|^6|^7",
        "illuminate/support": "^5.5|^6|^7",
        "illuminate/console": "^5.5|^6|^7",
        "illuminate/contracts": "^5.5|^6|^7",
        "illuminate/http": "^5.5|^6|^7",
        "illuminate/database": "^5.5|^6|^7",
        "illuminate/pagination": "^5.5|^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|>=8.3",
        "orchestra/testbench": ">=3.0",
        "phpunit/php-code-coverage": "^6.1.4|>=7"
    },
    "autoload": {
        "psr-4": {
            "ChurakovMike\\FormTypeCast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "run:unit": [
            "phpunit ./tests"
        ]
    }
}
